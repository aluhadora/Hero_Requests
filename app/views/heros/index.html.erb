<p id="notice"><%= notice %></p>

<h1>Listing Hero Requests</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @heros.each do |hero| %>
      <tr>
        <td><%= hero.name %></td>
        <td><%= link_to 'Remove from list (Done)', hero, method: :delete %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>